
/* pages/home/home.wxss */

.picker {
    padding: 16rpx;
}

.selected {
    border: 2rpx #434343 solid;
    border-radius: 20rpx;
    padding-top: 10rpx;
    padding-bottom: 10rpx;
    transition: 0.3s;
}

.not-selected{
    border: 2rpx transparent solid;
    padding: 10rpx;
}

.card-detail {
    margin-right: 5rpx;
    transition: 0.3s;
}
.vh100 {
    background-color: #F8F8F8;
}


