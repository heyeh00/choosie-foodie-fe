}

.card-l {
    width: 560rpx;
    border-radius: 50rpx;
    background-color: #FAEBCD;
  }

.info {
    padding: 30rpx;
}

.card-pic {
    object-fit: cover;
    width: 100%;
}

.name {
    font-size: larger;
    font-weight: 500;
}

.icon {
    width: 50rpx;
    height: 50rpx;
}

.info-row {
    font-size: medium;
    display: flex;
    align-items: center;
}

.attendees {
    font-size: medium;
    display: flex;
    align-items: center;
    flex-wrap: wrap;
}

.person {
    text-align: center;
    margin: 8rpx 12rpx;
}

.person-icon {
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
}