/**app.wxss**/
@import "/mp-stylesheets/index.wxss";

.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.our-button {
  text-align: center;
  border-radius: 40rpx;
  font-family: 'Raleway';
  font-style: normal;
  font-weight: 500;
  font-display: swap;
  background-color: #F7C873;
  color: #434343;
  border: 1rpx solid #434343; 
}

.card-l {
  width: 90%;
  height: 900rpx;
  border-radius: 50rpx;
  background-color: #FAEBCD;
  margin: auto;
}